<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="project_task_create_timesheet_view_form" model="ir.ui.view">
			<field name="name">attendance.create.timesheet.wizard.form</field>
			<field name="model">event.track.create.timesheet</field>
	        <field name="arch" type="xml">
		        <form>
					<group>
						<group>
							<field name="session_id"/>
		                    <field name="check_in" />
		                    <field name="check_out" />
		                    <field name="employee_id" />
		                </group>
		            </group>
		            <footer>
			            <button string="Save" type="object" name="save_timesheet" class="btn btn-primary" />
		            </footer>
		        </form>
	        </field>
	    </record>
    </data>
</odoo>
