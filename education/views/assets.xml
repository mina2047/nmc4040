<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="education_assets_backend" inherit_id="web.assets_backend" name="education assets backend">
        <xpath expr="script[last()]" position="after">
            <script type="text/javascript" src="/education/static/src/js/session_configurator_controller.js"></script>
            <script type="text/javascript" src="/education/static/src/js/session_configurator_view.js"></script>
            <script type="text/javascript" src="/education/static/src/js/session_configurator_widget.js"></script>
            <script type="text/javascript" src="/education/static/src/js/account_payment_field.js"></script>
        </xpath>
    </template>

    <template id="web_editor.assets_common" inherit_id="web.assets_common"
              name="Web Editor Common Assets (used in backend interface and website)">
        <xpath expr="script[last()]" position="after">
            <script type="text/javascript" src="/web_editor/static/lib/vkbeautify/vkbeautify.0.99.00.beta.js"/>
            <script type="text/javascript" src="/web_editor/static/src/js/common/ace.js"/>
        </xpath>
    </template>

</odoo>
