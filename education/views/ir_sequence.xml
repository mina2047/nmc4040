<odoo>
    <data>
        <record id="ir_sequence_form_inherit" model="ir.ui.view">
            <field name="name">ir.sequence.form.inherit</field>
            <field name="model">ir.sequence</field>
            <field name="inherit_id" ref="base.sequence_view" />
            <field name="arch" type="xml">

            	<xpath expr="//form[1]/sheet[1]/notebook[1]/page[1]/group[1]/group[2]/field[@name='number_next_actual']" position="after">
            		<field name="number"/>

            	 </xpath>
  
  
            </field>
        </record>

    </data>
</odoo>